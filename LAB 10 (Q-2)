CREATE TABLE COLLEGE(
eName VARCHAR(10),
state VARCHAR(10),
enrollment INT
);

CREATE TABLE STUDENT(
sID INT,
sName VARCHAR(10),
GPA NUMERIC(2,1),
sizeHS INT,
DoB DATE
);

CREATE TABLE APPLY(
sID INT,
eName VARCHAR(10),
major VARCHAR(20),
decision CHAR(1)
);

