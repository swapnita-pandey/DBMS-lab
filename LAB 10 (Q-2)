SELECT sName, DoB FROM STUDENT;

SELECT sName FROM STUDENT WHERE GPA > 3.7;

SELECT sName FROM STUDENT WHERE sizeHS > 1000 AND DoB >= '01-Jan-1996';

SELECT sName FROM STUDENT WHERE GPA BETWEEN 2.9 AND 3.9;

SELECT * FROM COLLEGE WHERE state = 'MA';

SELECT sName FROM STUDENT WHERE GPA BETWEEN 2.0 AND 3.5;

SELECT sName FROM STUDENT WHERE DoB > '01-Jul-1996' ORDER BY(DoB);

SELECT sID, eName, decision FROM APPLY WHERE decision = 'A';

SELECT sID, eName FROM APPLY WHERE eName = 'Stanford';

SELECT eName FROM COLLEGE WHERE enrollment > 10001;

SELECT eName FROM COLLEGE WHERE NOT state = 'California';

SELECT sName FROM STUDENT WHERE sizeHS > 1700 AND GPA < 3.8;

desc STUDENT;

SELECT *FROM STUDENT;

SELECT DISTINCT(major) FROM APPLY;

SELECT sName FROM STUDENT WHERE sNAME LIKE '___';

SELECT sName FROM STUDENT WHERE sName LIKE 'H____';

SELECT sName FROM STUDENT WHERE sName LIKE '__e_e%';

SELECT sName FROM STUDENT WHERE sName LIKE '%y';

SELECT sName, GPA FROM STUDENT ORDER BY(GPA);

SELECT *FROM STUDENT ORDER BY(GPA) ASC, DoB DESC;

SELECT sID FROM APPLY WHERE eName = 'Stanford' OR eName = 'Cornell' OR eName = 'MIT';

DELETE FROM APPLY WHERE eName = 'Stanford';

DELETE FROM COLLEGE WHERE eName = 'Stanford';

UPDATE STUDENT
SET GPA = GPA + 0.1*GPA WHERE GPA < 9;

